S_0x148804080 .scope module, "reg32_tb" "reg32_tb" 2 1;
 .timescale 0 0;
v0x600000648240_0 .var "t_clk", 0 0;
v0x6000006482d0_0 .var "t_in", 31 0;
v0x600000648360_0 .net "t_out", 31 0, v0x600000648120_0;  1 drivers
v0x6000006483f0_0 .var "t_res", 0 0;
S_0x1488041f0 .scope module, "uut" "reg32" 2 8, 3 1 0, S_0x148804080;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "res_n";
    .port_info 2 /INPUT 32 "in";
    .port_info 3 /OUTPUT 32 "out";
v0x600000648000_0 .net "clk", 0 0, v0x600000648240_0;  1 drivers
v0x600000648090_0 .net "in", 31 0, v0x6000006482d0_0;  1 drivers
v0x600000648120_0 .var "out", 31 0;
v0x6000006481b0_0 .net "res_n", 0 0, v0x6000006483f0_0;  1 drivers
E_0x600002140000 .event posedge, v0x600000648000_0;
    .scope S_0x1488041f0;
T_0 ;
    %wait E_0x600002140000;
    %load/vec4 v0x6000006481b0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x600000648120_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x600000648090_0;
    %assign/vec4 v0x600000648120_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x148804080;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x600000648240_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x148804080;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x600000648240_0;
    %inv;
    %store/vec4 v0x600000648240_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x148804080;
T_3 ;
    %vpi_call 2 19 "$dumpfile", "reg32_wave.vcd" {0 0 0};
    %vpi_call 2 20 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x148804080 {0 0 0};
    %vpi_call 2 21 "$monitor", "Time=%0t | Reset=%b | In=%d | Out=%d", $time, v0x6000006483f0_0, v0x6000006482d0_0, v0x600000648360_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6000006483f0_0, 0, 1;
    %pushi/vec4 100, 0, 32;
    %store/vec4 v0x6000006482d0_0, 0, 32;
    %delay 13, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6000006483f0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 200, 0, 32;
    %store/vec4 v0x6000006482d0_0, 0, 32;
    %delay 10, 0;
    %pushi/vec4 300, 0, 32;
    %store/vec4 v0x6000006482d0_0, 0, 32;
    %delay 5, 0;
    %delay 20, 0;
    %vpi_call 2 37 "$finish" {0 0 0};
    %end;
    .thread T_3;
