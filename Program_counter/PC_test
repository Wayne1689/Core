    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x600002480090_0;
    %assign/vec4 v0x600002480120_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x1357043d0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x600002480240_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x1357043d0;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x600002480240_0;
    %inv;
    %store/vec4 v0x600002480240_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x1357043d0;
T_3 ;
    %vpi_call 2 20 "$dumpfile", "pc_wave.vcd" {0 0 0};
    %vpi_call 2 21 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x1357043d0 {0 0 0};
    %vpi_call 2 22 "$monitor", "Time=%0t | Reset=%b | In=%d | Out=%d", $time, v0x6000024803f0_0, v0x6000024802d0_0, v0x600002480360_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6000024803f0_0, 0, 1;
    %pushi/vec4 100, 0, 32;
    %store/vec4 v0x6000024802d0_0, 0, 32;
    %delay 12, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6000024803f0_0, 0, 1;
    %delay 10, 0;
