v0x600002480120_0 .var "PC_output", 31 0;
v0x6000024801b0_0 .net "reset", 0 0, v0x6000024803f0_0;  1 drivers
E_0x600000384400 .event posedge, v0x600002480000_0;
    .scope S_0x135704540;
T_0 ;
    %wait E_0x600000384400;
    %load/vec4 v0x6000024801b0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 32;
